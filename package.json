{
  "name": "Duckling",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && concurrently \"npm run watch\" \"npm run duckling\" ",
    "test": "npm run build && concurrently \"npm run watch\" \"npm run duckling-test\" ",
    "build": "gulp && tsc",
    "watch": "concurrently \"gulp watch\" \"npm run tsc:w\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "duckling": "electron build/electron/background-main.js",
    "duckling-test": "electron build/duckling-tests/background-test-main.js",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "@angular2-material/card": "2.0.0-alpha.3",
    "@angular2-material/core": "2.0.0-alpha.3",
    "@angular2-material/input": "2.0.0-alpha.3",
    "@angular2-material/list": "2.0.0-alpha.3",
    "angular2": "2.0.0-beta.14",
    "es6-shim": "^0.35.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.2.0",
    "gulp-watch": "^4.3.5",
    "immutable": "^3.8.1",
    "pixi.js": "^3.0.11",
    "jasmine-core": "^2.4.1",
    "redux": "^3.5.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "system": "^1.0.4",
    "systemjs": "0.19.25",
    "typescript": "^1.8.10",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "electron-prebuilt": "^0.36.0",
    "concurrently": "^2.0.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12"
  }
}
