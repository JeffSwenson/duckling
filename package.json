  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && concurrently \"npm run watch\" \"npm run duckling\" ",
    "test": "tsc && npm run mocha",
    "build": "gulp && tsc",
    "watch": "concurrently \"npm run gulp:w\" \"npm run tsc:w\"",
    "gulp": "gulp",
    "gulp:w": "gulp watch",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "mocha": "electron-mocha --renderer \"build/duckling-tests/**/*.spec.js\"",
    "mocha-debug": "electron-mocha --interactive \"build/duckling-tests/**/*.spec.js\"",
    "duckling": "electron build/electron/background-main.js"
  },
