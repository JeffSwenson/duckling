{
  "name": "Duckling",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build && concurrently \"npm run watch\" \"npm run duckling\" ",
    "test": "tsc && npm run mocha",
    "build": "gulp && tsc",
    "watch": "concurrently \"npm run gulp:w\" \"npm run tsc:w\"",
    "gulp": "gulp",
    "gulp:w": "gulp watch",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "mocha": "electron-mocha --renderer \"build/duckling-tests/**/*.spec.js\"",
    "duckling": "electron build/electron/background-main.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "@angular/upgrade": "^2.1.2",
    "@types/electron": "^1.4.26",
    "@types/es6-shim": "0.31.32",
    "@types/glob": "^5.0.30",
    "@types/hammerjs": "^2.0.33",
    "@types/lodash": "^4.14.38",
    "@types/node": "^6.0.46",
    "@types/pixi.js": "^3.0.30",
    "@types/webfontloader": "^1.6.27",
    "es6-shim": "^0.35.1",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "gulp-watch": "^4.3.10",
    "immutable": "^3.8.1",
    "lodash": "^4.17.0",
    "open-iconic": "^1.1.1",
    "pixi.js": "^4.1.1",
    "redux": "^3.6.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
