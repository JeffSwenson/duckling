form
    label Collision Type:&nbsp;
    div
        select.selectpicker(rv-value="data.info.type | formatEnum 'entityframework.components.CollisionType'")
            option(rv-each-type="typeVals | formatEnumVals 'entityframework.components.CollisionType'") { type }
    br

    label Body Type:&nbsp;
    div
        select.selectpicker(rv-value="data.info.bodyType | formatEnum 'entityframework.components.CollisionBodyType'")
            option(rv-each-type="bodyTypeVals | formatEnumVals 'entityframework.components.CollisionBodyType'") { type }
    br

    label Collision Dimensions
    div.form-group
        div.input-group
            div.input-group-addon.width-height-input-group Width
            input.form-control(type="text" placeholder="Collision Width" rv-value="data.info.dimensions.x")
        div.input-group
            div.input-group-addon.width-height-input-group Height
            input.form-control(type="text" placeholder="Collision Height" rv-value="data.info.dimensions.y")
